{
    "compilerOptions": {
      "allowJs": true,
      "module": "esnext",
      "moduleResolution": "node",
      "noEmit": true,
      "pretty": true,
      "resolveJsonModule": true,
      "strict": true,
      "target": "esnext",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "lib": [
        "esnext"
      ],
      "plugins": [
        {
          "name": "typescript-deno-plugin"
        }
      ]
    }
  }